# Environment variables for Strategic Intelligence Platform (Vite)
# Copy this file to .env and fill in your Supabase project details.

# Supabase project URL (e.g., https://your-project-id.supabase.co)
VITE_SUPABASE_URL=

# Supabase anon public key
VITE_SUPABASE_ANON_KEY=

# Local analysis mode (bypass backend and use local engine)
VITE_LOCAL_ANALYZE=false
